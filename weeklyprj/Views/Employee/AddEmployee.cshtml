@model weeklyprj.Models.Employee
    <h2>Add Employee</h2>

    <form asp-action="AddEmployee" method="post">
        <div>
            Name:   
            <input asp-for="Name"/>
        </div>

        <div>
            Salary:
            <input asp-for="Salary" />
        </div>

        <br>

        <button type="submit">Add</button>
    </form>





    @model Weeklyprep.Models.Student

<form asp-action="AddStudent" method="Post">
    <label asp-for="Name">Name</label>
    <input asp-for="Name"/>

    <label asp-for="Course">Course</label>
    <input asp-for="Course"/>

    <label asp-for="Year">Year</label>
    <input asp-for="Year"/>
    <span asp-validation-for="Year"></span>

    <label asp-for="GPA">GPA</label>
    <input asp-for="GPA"/>
    <span asp-validation-for="GPA"></span>




</form>